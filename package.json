{
  "name": "tmp-cpa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "keystone:start": "keystone start",
    "keystone:build": "keystone build",
    "prisma": "keystone prisma",
    "postinstall": "keystone postinstall",
    "lint": "eslint src --ext .tsx,.ts",
    "commit": "git-cz",
    "lint:fix": "npm run lint -- --fix",
    "codegen": "DOTENV_CONFIG_PATH=.env.local npx graphql-codegen -r dotenv/config --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@graphql-codegen/typescript": "^2.8.3",
    "@graphql-codegen/typescript-operations": "^2.5.8",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@keystone-6/auth": "^6.0.0",
    "@keystone-6/core": "^4.0.1",
    "@keystone-6/fields-document": "^6.0.0",
    "graphql": "^16.6.0",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@graphql-codegen/cli": "^2.15.0",
    "@rocketseat/eslint-config": "^1.1.3",
    "commitizen": "^4.2.6",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "dotenv": "^16.0.3",
    "typescript": "4.9.3",
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint-config-next": "13.0.6"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "src/**": "npm run lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
